   "metadata": {},
   "outputs": [],
   "source": [
    "# 재귀적의 의미를 코드로 풀어본것.\n",
    "\n",
    "def calc_sum(num):\n",
    "    if num == 1:\n",
    "        return num\n",
    "    return num + calc_sum(num-1)"
   ]
  },
